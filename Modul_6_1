#include <stdio.h>

int main() {
    printf("Deret Bilangan Genap dari 0 hingga 50:\n");
    for (int i = 0; i <= 50; i++) {
        if (i % 2 == 0) {  
            printf("%d ", i);
        }
    }

    printf("\n\n");

    printf("Deret Bilangan Ganjil dari 0 hingga 50:\n");
    for (int i = 0; i <= 50; i++) {
        if (i % 2 != 0) {  /
            printf("%d ", i);
        }
    }

    return 0;
}
